<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" 
          xmlns="http://maven.apache.org/SETTINGS/1.1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <id>bidms-internal</id>
      <username>bidms-build</username>
      <password>BIDMS_BUILD_PW</password>
    </server>
    <server>
      <id>bidms-internal-snapshots</id>
      <username>bidms-build</username>
      <password>BIDMS_BUILD_PW</password>
    </server>
  </servers>
  <profiles>
    <profile>
      <id>archivaProfile</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <repositories>
        <repository>
          <id>bidms-internal</id>
          <url>https://bidms-tomcat-dev:8560/repository/internal</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>bidms-internal-snapshots</id>
          <url>https://bidms-tomcat-dev:8560/repository/snapshots</url>
          <releases>
            <enabled>false</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>bidms-internal</id>
          <url>https://bidms-tomcat-dev:8560/repository/internal</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </pluginRepository>
        <pluginRepository>
          <id>bidms-internal-snapshots</id>
          <url>https://bidms-tomcat-dev:8560/repository/snapshots</url>
          <releases>
            <enabled>false</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
</settings>
